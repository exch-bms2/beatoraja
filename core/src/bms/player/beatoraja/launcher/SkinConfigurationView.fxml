<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bms.player.beatoraja.launcher.SkinConfigurationView">
    <HBox prefHeight="23.0" prefWidth="723.0">
        <VBox.margin>
            <Insets bottom="10.0" left="10.0" top="10.0"/>
        </VBox.margin>
        <Label prefHeight="24.0" prefWidth="100.0" text="%Category"/>
        <ComboBox fx:id="skintypeSelector" onAction="#changeSkinType" prefWidth="150.0"/>
        <Label prefHeight="24.0" prefWidth="60.0" text="%Skin">
            <HBox.margin>
                <Insets left="20.0"/>
            </HBox.margin>
        </Label>
        <ComboBox fx:id="skinheaderSelector" onAction="#changeSkinHeader" prefHeight="24.0" prefWidth="294.0"/>
        <Button mnemonicParsing="false" onAction="#commitSkinType" text="%Update">
            <HBox.margin>
                <Insets left="20.0"/>
            </HBox.margin>
        </Button>
    </HBox>
    <ScrollPane fx:id="skinconfig" prefHeight="400.0" prefWidth="723.0" VBox.vgrow="ALWAYS">
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0"/>
    </ScrollPane>
</VBox>
